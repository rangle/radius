{
  "name": "radius",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "main": "index.js",
  "scripts": {
    "add": "lerna add",
    "version": "lerna version",
    "publish": "lerna publish from-package --yes --registry http://localhost:4873/",
    "build": "nx run-many --target=build --all --parallel",
    "graph": "npx nx graph",
    "registry": "verdaccio",
    "local:publish": "lerna publish  from-package --yes --force-publish --no-push --no-git-reset --registry http://localhost:4873/ ",
    "local:unpublish": "lerna exec -- npm unpublish --registry http://localhost:4873/  \"\\${LERNA_PACKAGE_NAME}@\\$(npm view \\$LERNA_PACKAGE_NAME version)\"",
    "local:unpublish-all": "lerna exec -- npm unpublish -f --registry http://localhost:4873/ \"\\${LERNA_PACKAGE_NAME}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rangle/radius.git"
  },
